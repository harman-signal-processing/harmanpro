(function(){
	var $list_of_associated_data_clients = $('#contact_data_clients');
	build_list_of_data_clients_associated_with_this_contact($list_of_associated_data_clients);
	
	function build_list_of_data_clients_associated_with_this_contact($list_of_associated_data_clients) {
		<% @contact_data_clients.each do |contact_data_client| %>
			$list_of_associated_data_clients.append('<%= escape_javascript(render "/contact_info/admin/contact_data_clients/contact_data_client_row", association: contact_data_client, called_from: @called_from) %>');
		<% end %>	
	}  //  function build_list_of_data_clients_associated_with_this_contact($list_of_associated_data_clients)
})();