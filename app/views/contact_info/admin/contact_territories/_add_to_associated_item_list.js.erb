(function(){
	var $list_of_associated_territories = $('#contact_territories');
	build_list_of_territories_associated_with_this_contact($list_of_associated_territories);
	
	function build_list_of_territories_associated_with_this_contact($list_of_associated_territories) {
		<% @contact_territories.each do |contact_territory| %>
			$list_of_associated_territories.append('<%= escape_javascript(render "/contact_info/admin/contact_territories/contact_territory_row", association: contact_territory, called_from: @called_from) %>');
		<% end %>	
	}  //  function build_list_of_territories_associated_with_this_contact($list_of_associated_territories)
})();