<ul class="button-group round">
    <li><%= link_to 'Websites List', contact_info_admin_websites_path, class: "small button round" %>	</li>
    <li><%= link_to 'Delete', contact_info_admin_website_path(@website), confirm: 'Are you sure?', method: :delete, class: "small alert button" %> </li>
</ul>
<h1>Edit Website</h1>

<%= simple_form_for @website, url: contact_info_admin_website_path do |f| %>
  <%= f.input :url, label: "URL" %>
  <%= f.input :label, label: "Label" %>

    </br>
  <%= f.button :submit %>
<% end # simple_form_for @website, url: contact_info_admin_website_path do |f|%>


