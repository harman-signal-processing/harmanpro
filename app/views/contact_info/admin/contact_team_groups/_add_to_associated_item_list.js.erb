(function(){
	var $list_of_associated_team_groups = $('#contact_team_groups');
	build_list_of_team_groups_associated_with_this_contact($list_of_associated_team_groups);
	
	function build_list_of_team_groups_associated_with_this_contact($list_of_associated_team_groups) {
		<% @contact_team_groups.each do |contact_team_group| %>
			$list_of_associated_team_groups.append('<%= escape_javascript(render "/contact_info/admin/contact_team_groups/contact_team_group_row", association: contact_team_group, called_from: @called_from) %>');
		<% end %>	
	}  //  function build_list_of_team_groups_associated_with_this_contact($list_of_associated_team_groups)
})();