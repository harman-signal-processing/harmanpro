(function(){
	var $list_of_associated_emails = $('#contact_emails');
	build_list_of_emails_associated_with_this_contact($list_of_associated_emails);
	
	function build_list_of_emails_associated_with_this_contact($list_of_associated_emails) {
		<% @contact_emails.each do |contact_email| %>
			$list_of_associated_emails.append('<%= escape_javascript(render "/contact_info/admin/contact_emails/contact_email_row", association: contact_email, called_from: @called_from) %>');
		<% end %>	
	}  //  function build_list_of_emails_associated_with_this_contact($list_of_associated_emails)
})();