(function(){
	var $list_of_associated_locations = $('#distributor_locations');
	build_list_of_locations_associated_with_this_distributor($list_of_associated_locations);
	
	function build_list_of_locations_associated_with_this_distributor($list_of_associated_locations) {
		<% @distributor_locations.each do |distributor_location| %>
			$list_of_associated_locations.append('<%= escape_javascript(render "/distributor_info/admin/distributor_locations/distributor_location_row", association: distributor_location, called_from: @called_from) %>');
		<% end %>	
	}  //  function build_list_of_locations_associated_with_this_distributor($list_of_associated_locations)
})();