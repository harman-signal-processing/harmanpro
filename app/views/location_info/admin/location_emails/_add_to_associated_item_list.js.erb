(function(){
	var $list_of_associated_emails = $('#location_emails');
	build_list_of_emails_associated_with_this_location($list_of_associated_emails);
	
	function build_list_of_emails_associated_with_this_location($list_of_associated_emails) {
		<% @location_emails.each do |location_email| %>
			$list_of_associated_emails.append('<%= escape_javascript(render "/location_info/admin/location_emails/location_email_row", association: location_email, called_from: @called_from) %>');
		<% end %>	
	}  //  function build_list_of_emails_associated_with_this_location($list_of_associated_emails)
})();