(function(){
	var $list_of_associated_contacts = $('#location_contacts');
	build_list_of_contacts_associated_with_this_location($list_of_associated_contacts);
	
	function build_list_of_contacts_associated_with_this_location($list_of_associated_contacts) {
		<% @location_contacts.each do |location_contact| %>
			$list_of_associated_contacts.append('<%= escape_javascript(render "/location_info/admin/location_contacts/location_contact_row", association: location_contact, called_from: @called_from) %>');
		<% end %>	
	}  //  function build_list_of_contacts_associated_with_this_location($list_of_associated_contacts)
})();