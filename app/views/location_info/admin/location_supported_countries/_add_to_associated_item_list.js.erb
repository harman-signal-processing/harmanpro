(function(){
	var $list_of_associated_supported_countries = $('#location_supported_countries');
	build_list_of_supported_countries_associated_with_this_location($list_of_associated_supported_countries);
	
	function build_list_of_supported_countries_associated_with_this_location($list_of_associated_supported_countries) {
		<% @location_supported_countries.each do |location_supported_country| %>
			$list_of_associated_supported_countries.append('<%= escape_javascript(render "/location_info/admin/location_supported_countries/location_supported_country_row", association: location_supported_country, called_from: @called_from) %>');
		<% end %>	
	}  //  function build_list_of_supported_countries_associated_with_this_location($list_of_associated_supported_countries)
})();