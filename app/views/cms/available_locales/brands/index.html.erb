<div class="large-12 small-12 columns">
  <%= render 'cms/available_locales/side_nav' %>

  <div class="medium-10 small-12 columns">
    <h3>Choose a brand:</h3>
    <ul>
      <% @brands.order(:name).each do |brand| %>
        <li> <%= link_to brand.name, edit_cms_available_locale_brand_path(@available_locale, brand) %> </li>
      <% end %>
    </ul>
  </div>
</div>
